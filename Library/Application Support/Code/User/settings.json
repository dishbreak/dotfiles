{
  "redhat.telemetry.enabled": true,
  "githubPullRequests.pullBranch": "never",
  "[terraform]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform",
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "file"
  },
  "terraform.languageServer.enable": true,
  "[python]": {
    "editor.formatOnType": true
  },
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "aws.samcli.location": "/opt/homebrew/bin/sam",
  "aws.resources.enabledResources": [
    "AWS::IAM::Role"
  ],
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "git.confirmSync": false,
  "vs-kubernetes": {
    "vscode-kubernetes.helm-path.mac": "/Users/vishal/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.minikube-path.mac": "/Users/vishal/.vs-kubernetes/tools/minikube/darwin-arm64/minikube",
    "vs-kubernetes.minikube-show-information-expiration": "2023-08-17T15:01:01.908Z",
    "vscode-kubernetes.helm-path-mac": "/Users/vishal/.vs-kubernetes/tools/helm/darwin-arm64/helm",
    "vscode-kubernetes.minikube-path-mac": "/Users/vishal/.vs-kubernetes/tools/minikube/darwin-arm64/minikube",
    "vs-kubernetes.crd-code-completion": "disabled"
  },
  "vscode-kubernetes.log-viewer.follow": true,
  "vscode-kubernetes.log-viewer.timestamp": true,
  "vscode-kubernetes.log-viewer.since": -1,
  "vscode-kubernetes.log-viewer.tail": -1,
  "vscode-kubernetes.log-viewer.destination": "Webview",
  "vscode-kubernetes.log-viewer.wrap": false,
  "githubPullRequests.fileListLayout": "tree",
  "githubPullRequests.pushBranch": "always",
  "go.toolsManagement.autoUpdate": true,
  "editor.inlayHints.enabled": "off",
  "workbench.colorCustomizations": {
    "editor.lineHighlightBackground": "#1073cf2d",
    "editor.lineHighlightBorder": "#9fced11f"
  },
  "editor.wordWrap": "off",
  "diffEditor.wordWrap": "off",
  "editor.guides.indentation": false,
  "editor.guides.bracketPairs": false,
  "blockman.n04Sub01ColorComboPresetForDarkTheme": "Small Galaxy (Solid colors)",
  "workbench.colorTheme": "Zenburn",
  "files.autoSave": "afterDelay",
  "security.workspace.trust.untrustedFiles": "open",
  "github.copilot.enable": {
    "*": false,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "python": true
  },
  "go.lintTool": "golangci-lint",
  "git.blame.editorDecoration.enabled": true,
  "terminal.integrated.copyOnSelection": true
}
